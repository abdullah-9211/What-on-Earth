<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Asia">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/border">
        <ImageButton
            android:id="@+id/back4"
            android:layout_width="35dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/ic_baseline_arrow_back_24" />


        <TextView
            android:id="@+id/C02ConcentrationStat5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_alignParentTop="true"
            android:fontFamily="@font/concertone"
            android:text="C02 Concentration"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"/>
        <ProgressBar
            android:id="@+id/C02ConcentrationStatbar5"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/C02ConcentrationStat5"
            style="?android:attr/progressBarStyleHorizontal"
            android:progressDrawable="@drawable/progress"
            android:layout_marginLeft="20dp"
            android:indeterminate="false"
            android:max="100"
            android:minHeight="40dp"
            android:minWidth="200dp"
            android:progress="50" />
        <TextView
            android:id="@+id/SeaLevelStat5"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:fontFamily="@font/concertone"
            android:text="Sea Level"
            android:layout_below="@id/C02ConcentrationStatbar5"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"/>
        <ProgressBar
            android:id="@+id/SeaLevelStatbar5"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/SeaLevelStat5"
            style="?android:attr/progressBarStyleHorizontal"
            android:progressDrawable="@drawable/progress"
            android:layout_marginLeft="20dp"
            android:indeterminate="false"
            android:max="100"
            android:minHeight="40dp"
            android:minWidth="200dp"
            android:progress="50" />

        <TextView
            android:id="@+id/AirPollutionStat5"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/SeaLevelStatbar5"
            android:fontFamily="@font/concertone"
            android:text="Air pollution"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"/>

        <ProgressBar
            android:id="@+id/AirPollutionStatbar5"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/AirPollutionStat5"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="-1dp"
            android:indeterminate="false"
            android:max="100"
            android:minWidth="200dp"
            android:minHeight="40dp"
            android:progress="100"
            android:progressDrawable="@drawable/progress" />

        <TextView
            android:id="@+id/OceanAcidificationStat5"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:fontFamily="@font/concertone"
            android:text="Ocean Acidification"
            android:layout_below="@+id/AirPollutionStatbar5"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"/>
        <ProgressBar
            android:id="@+id/OceanAcidificationStatbar5"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:progressDrawable="@drawable/progress"
            android:layout_below="@id/OceanAcidificationStat5"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_marginLeft="20dp"

            android:indeterminate="false"
            android:max="100"
            android:minHeight="40dp"
            android:minWidth="200dp"
            android:progress="70" />
        <TextView
            android:id="@+id/SocialInequityStat5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/concertone"
            android:layout_alignParentTop="true"
            android:layout_marginTop="40dp"
            android:text="Social Inequity"
            android:layout_marginHorizontal="20dp"
            android:layout_alignParentEnd="true"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginEnd="20dp"
            android:layout_marginLeft="20dp"/>
        <ProgressBar
            android:id="@+id/SocialInequityStatbar5"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:progressDrawable="@drawable/progress"
            android:layout_below="@id/SocialInequityStat5"
            android:layout_marginEnd="20dp"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_marginLeft="20dp"
            android:indeterminate="false"
            android:max="100"
            android:minHeight="40dp"
            android:minWidth="200dp"
            android:progress="50" />
        <TextView
            android:id="@+id/BiodiversityStat5"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:fontFamily="@font/concertone"
            android:text="Loss of BioDiversity"
            android:layout_marginEnd="20dp"
            android:layout_below="@id/SocialInequityStatbar5"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"/>
        <ProgressBar
            android:id="@+id/BiodiversityStatbar5"
            android:layout_width="150dp"
            android:layout_alignParentEnd="true"
            android:layout_height="wrap_content"
            android:progressDrawable="@drawable/progress"
            android:layout_below="@id/BiodiversityStat5"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_marginLeft="20dp"
            android:layout_marginEnd="20dp"
            android:indeterminate="false"
            android:max="100"
            android:minHeight="50dp"
            android:minWidth="200dp"
            android:progress="50" />

        <TextView
            android:id="@+id/GreenHouseGasConcentrationStat5"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/BiodiversityStatbar5"
            android:layout_alignParentEnd="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="-1dp"
            android:layout_marginEnd="19dp"
            android:fontFamily="@font/concertone"
            android:text="GreenHouse Gas Concentration"
            android:textColor="@color/white"
            android:textSize="20sp" />

        <ProgressBar
            android:id="@+id/GreenHouseGasConcentrationStatbar5"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="20dp"
            android:progressDrawable="@drawable/progress"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/GreenHouseGasConcentrationStat5"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_marginLeft="20dp"
            android:indeterminate="false"
            android:max="100"
            android:minHeight="40dp"
            android:minWidth="200dp"
            android:progress="100" />
        <TextView
            android:id="@+id/extremeWeatherEventsStat5"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:fontFamily="@font/concertone"
            android:text="Extreme Weather Events"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/GreenHouseGasConcentrationStatbar5"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"/>
        <ProgressBar
            android:id="@+id/extremeWeatherStatbar5"
            android:layout_width="150dp"
            android:layout_alignParentEnd="true"
            android:progressDrawable="@drawable/progress"
            android:layout_marginEnd="20dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/extremeWeatherEventsStat5"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_marginLeft="20dp"

            android:indeterminate="false"
            android:max="100"
            android:minHeight="40dp"
            android:minWidth="200dp"
            android:progress="70" />

        <ImageView
            android:id="@+id/NAimage"
            android:layout_width="229dp"
            android:layout_height="222dp"
            android:layout_centerInParent="true"
            android:background="@drawable/img_5"
            android:translationY="-20dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/NAimage"
            android:layout_marginTop="60dp"
            android:text="Asia"
            android:layout_centerHorizontal="true"
            android:textColor="@color/white"
            android:textSize="40dp"
            android:translationY="-10dp"
            android:fontFamily="@font/concertone"/>



    </RelativeLayout>

</FrameLayout>